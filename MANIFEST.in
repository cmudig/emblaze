include LICENSE.txt
include README.md

include setupbase.py
include pytest.ini
include .coverage.rc

include tsconfig.json
include package.json
include webpack.config.js
graft emblaze/labextension

# Documentation
graft docs
exclude docs/\#*
prune docs/build
prune docs/gh-pages
prune docs/dist

# Examples
graft examples
prune examples/screenshots
graft emblaze/data
graft emblaze/public

# Tests
graft tests
prune tests/build

# Javascript files
graft emblaze/nbextension
graft src
graft css
prune **/node_modules
prune coverage
prune lib

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .DS_Store
global-exclude .git
prune **/.ipynb_checkpoints
